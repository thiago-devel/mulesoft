        Authorization:
          type: string
          description: |
            Used to send a valid OAuth2 access token.

          example: Bearer access_token
          required: true
        Accept-Language:
          type: string
          description: |
            Identifies the preferred language for the order related emails.
            Currently only the language with the highest quality value is used. If there are several
            languages with the same (highest) quality values, the first one is used.

          example: de, en-gb;q=0.8, en;q=0.7
          required: false
        hybris-site:
          type: string
          description: |
            The site on which the order is placed.

          example: Germany
          required: false
